function(my_lib_import_BackEndDLL _target)

   target_link_libraries(${_target} BackEndDLL)

endfunction()

function(my_inc_import_BackEndDLL _target)

   target_include_directories(${_target} PRIVATE $<TARGET_PROPERTY:BackEndDLL,INTERFACE_INCLUDE_DIRECTORIES>)

endfunction()

